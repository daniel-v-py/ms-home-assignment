{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westeurope"
        }, 
        "vmCount":{
            "value": 1
        },
        "vmNameprefix": {
            "value": "myfirstVMtemplate-"
        },
        "vmSize": {
            "value": "Standard_D2s_v3"
        },
        "storageAccountType": {
            "value": "Standard_LRS"
        },
        "adminUsername": {
            "value": "myusername"
